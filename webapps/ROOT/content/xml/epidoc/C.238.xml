                    <lb n="1"/><persName type="emperor" key="livia"><gap reason="lost" extent="unknown" unit="character"/> <name nymRef="Σεβαστή"><unclear>Σ</unclear>εβαστ<unclear>ή</unclear></name> <gap reason="lost" extent="unknown" unit="character"/></persName>
