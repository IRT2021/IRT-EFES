            <lb n="7"/> <space quantity="1" unit="character"/> <w lemma="ὁ">τὰν</w> <w lemma="γυνή">γ<space quantity="1" unit="character"/>υναῖκα</w> <space quantity="2" unit="character"/>
                </ab>
