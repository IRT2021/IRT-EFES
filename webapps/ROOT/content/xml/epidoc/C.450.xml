<ab>
                   
                    <lb n="1"/><persName type="divine" key="demeter"><name nymRef="Δημήτηρ">Δάμ<supplied reason="lost">ατρι</supplied></name></persName>
          <lb n="2"/> <w lemma="καί">καὶ</w> <persName type="divine" key="kore"><name nymRef="Κόρη">Κό<supplied reason="lost">ραι</supplied></name></persName>             
