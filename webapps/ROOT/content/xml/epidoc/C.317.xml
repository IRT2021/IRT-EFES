                    <lb n="2"/><space quantity="5" unit="character"/> <w lemma="καί">καὶ</w> <space quantity="1" unit="character"/> <persName type="divine" key="nymphae"><name nymRef="Νύμφη">Νύμφαις</name></persName> <space quantity="3" unit="character"/>
