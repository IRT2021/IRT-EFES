                    <lb n="3"/><persName type="attested"><name nymRef="Ἰάσων">Ἰάϲων</name></persName>
                    <lb n="4"/> <w lemma="ἀποθνήσκω">ἀπέθα<lb n="5" break="no"/>νε</w> <date type="life-span" dur="P48Y"><w lemma="ἔτος"><expan><abbr><am><g ref="symbols.xml#year"/></am></abbr><ex>ἐτῶν</ex></expan></w>
                    <num value="48">μ<hi rend="subscript">η</hi></num> </date>
