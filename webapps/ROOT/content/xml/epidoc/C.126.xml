                    <lb n="1"/><w lemma="pro">pro</w> <w lemma="salus">salute</w>
               <persName type="attested" key="pir2-p-0756"><name nymRef="Publius"><expan><abbr>P</abbr><ex>ubli</ex></expan></name>
                  <name nymRef="Pomponius">Pomponi</name> <name nymRef="Secundus">Secundi</name></persName> 
                    <lb n="1"/> <space quantity="3" unit="character"/>
              <w lemma="proconsul"><expan><abbr>pro<space quantity="1" unit="character"/>co</abbr><ex>n</ex><abbr>s</abbr><ex>ulis</ex></expan></w>
