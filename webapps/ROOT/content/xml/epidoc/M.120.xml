         <div type="edition" xml:lang="grc" xml:space="preserve">
            <ab>
                    <lb n="1"/><g ref="symbols.xml#crux"/> <w lemma="ὑπέρ">ὑπὲρ</w> <w>εὐχῆϲ</w> <w lemma="καί">κὲ</w>
                    <lb n="2"/> <w>ϲωτηρίαϲ</w> <persName type="attested" key="lgpn:V1-60731"><name nymRef="Ἰοῦστος">Ἰούϲτ<supplied reason="lost">ου</supplied></name></persName>       
                    <lb n="3"/> <w lemma="καί">καὶ</w> <persName type="attested" key="lgpn:V1-60738"><name nymRef="Μηνᾶς">Μηνᾶ</name></persName>
                           <w>ἀναγ<supplied reason="lost">ν</supplied><unclear>ω</unclear>
                    <lb n="4" break="no"/>ϲτ<unclear>ῶν</unclear></w>
                        <w lemma="οἰκόδομος">κὐκ<unclear>ο</unclear>δώ<unclear>μον</unclear></w>
                    <lb n="5"/> <w lemma="ἀμήν">ἀμὴν</w> <persName type="divine" key="iesous"><name nymRef="Χριστός">Χριϲ<supplied reason="lost">τέ</supplied></name></persName>
                </ab>
         </div>
         
         <div type="apparatus">
            <listApp><app loc="3"><rdg resp="#jmr1960, #gpc1961">ω <note>om.</note></rdg></app>
               
               <app loc="4"><rdg resp="#gpc1961">ΚΥΚΙΔΩΜΕΝ[ . . . <note>Pugliese-Carratelli suggested  κυρίο]υ κιδωμέν[ων] = κηδομένων</note></rdg>
               <rdg resp="#jmr1960">κυκοωμέν[ων] <note>(? for κεκοιμένων)</note></rdg>
                  <rdg resp="#jmr-jbwp-rgg2003">ΚΥΚΟΔΩΜΟΝ[ . . ]</rdg>
                  <rdg resp="#cdlBE">κὐκοκοδόμον <note>for κὲ οἰκοδόμων</note></rdg></app></listApp>
