                  <ptr target="PHI"/> <citedRange><ref target="https://epigraphy.packhum.org/text/324464">324464</ref></citedRange></bibl>;
                mentioned  
               <bibl n="seg"><ptr target="SEG"/> <citedRange>20.764</citedRange></bibl>;
