         <div type="apparatus">
            <head>Apparatus</head>
            <listApp>
               <app loc="1">
                  <rdg>Μ could be ΝΝ in ligature</rdg>
               </app>
               <app loc="1">
                  <rdg>Σ is reversed</rdg>
               </app>
            </listApp>
