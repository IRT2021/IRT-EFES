             <div subtype="fragment" n="a" type="textpart"><ab>
                    <lb n="1"/><orig>ΠΡΙ<unclear>Μ</unclear>ΟΔΨ</orig>
                    <lb n="2"/><orig>ΑΒΓΦ<unclear>Ρ</unclear>ΣΟ</orig>
                  </ab>
             </div>
            <div subtype="fragment" n="b" type="textpart">
                     <ab>
                    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><orig>ο<unclear>ϲ</unclear>Ι</orig><gap reason="lost" extent="unknown" unit="character"/>
